#ifndef GEFRAN_H
#define GEFRAN_H

#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
#include <unistd.h>
#include <stdint.h>
#include <string.h>

#include <net/if.h>
#include <sys/types.h>
#include <sys/socket.h>
#include <sys/ioctl.h>

#include <linux/can.h>
#include <linux/can/raw.h>

static struct can_frame gefran_can_frame;
static  uint8_t status;
/**
 * Значения положения угла в HEX  
 */

typedef struct _position_hex{
  __u16 byte1;
  __u16 byte2;
  int16_t X_hex_result;
  int16_t Y_hex_result;
} position_hex;

/**
 * @brief 
 * Открывай CAN интерфейс 
 * @param can 
 * @return true 
 * @return false 
 */
static bool open_interfeice(char** can);
/**
 * argument: пакет CAN сообщения
 * return: возвращает первый и второй байт в виде строки
 */

static _position_hex* give_value_position();


#endif
